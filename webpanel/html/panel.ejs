 <div class="content" style="background-color: #363940;">
<div class="container-fluid">

<center>
<div class="row">
<div class="col-md-12">
<div class="card" style="background: #202225;">
<div class="card-header card-header-text card-header-primary">
<div class="card-text">
<h4 class="card-title"><%=user.guilds.size%> Sunucu Yönetebilirsiniz.</h4>
</div>
</div>
<div class="card-body">
<% user.guilds.forEach((sunucu) => {
        const permsOnGuild = new perms(sunucu.permissions);
        if(!permsOnGuild.has("MANAGE_GUILD")) return;
      %>
      <%
      let link, eklenmemis;
      if (bot.guilds.has(sunucu.id) === true) {
        link = `/yonet?sunucu=${sunucu.id}`
      } else {
        link = `/ekle`
        eklenmemis = true
      }
      %>
      <% if (sunucu.icon) { %>
      <a href="<%=link%>" target="<%if(eklenmemis === true) {%>_blank<%} else {  }%>"><img title="<%=sunucu.name%>"  src="<%= `https://cdn.discordapp.com/icons/${sunucu.id}/${sunucu.icon}.png?size=128` %>" width="120" height="120" alt="" class="pic img-circle image aw-zoom"></a>
		  <% } else { %>
      <a href="<%=link%>"><img title="<%=sunucu.name%>" src="https://www.mcadventuretime.com/discord/assets/img/discordimage.png" id="<%= sunucu.name.split(' ').join('').replace(`'`, ``) %>" width="120" height="120" alt="" class="pic img-circle image aw-zoom"></img></a>
		  <script>
        function draw() {
        const ctx = document.getElementById("<%= sunucu.name.split(' ').join('').replace(`'`, ``) %>").getContext('2d');
			  ctx.font = '20px Arial';
			  ctx.textAlign = "center";
			  ctx.fillStyle = "white";
			  ctx.fillText("<%= sunucu.name.split(' ').map(v => v[0]).join('') %>", 40, 48);
        document.getElementById("<%= sunucu.name.split(' ').join('').replace(`'`, ``) %>").style.marginBottom = "-35px";
        };
        draw();
		  </script>
		<% } %>
    <% }); %>
</div>
</div>
</div>
</div>
</center></div>

<div style="color: white; background-color: #363940;">
  <br>
  <center><h4>Merhaba! Sunucu Yönet yetkisine sahip olduğunuz tüm sunucular listelendi!</h4></center>
